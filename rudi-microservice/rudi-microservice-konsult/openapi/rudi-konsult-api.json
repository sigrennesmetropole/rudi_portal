				"parameters": [
					{
						"$ref": "#/components/parameters/GlobalIdPathParam"
					},
					{
						"in": "query",
						"name": "limit",
						"description": "Le nombre de résultats souhaité",
						"schema": {
							"type": "integer",
							"format": "int32"
						}
					}
				],
				"responses": {
					"200": {
						"description": "",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"type": "Metadata"
									}
								}
							}
						}
					},
					"500": {
						"$ref": "#/components/responses/InternalServerError"
					}
				}
			}
		},

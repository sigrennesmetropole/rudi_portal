{
	"openapi": "3.0.0",
	"info": {
		"description": "API Public Template Rudi",
		"version": "0.0.1",
		"title": "rudi-template-api",
		"termsOfService": "https://swagger.io/terms/",
		"contact": {
			"email": "rudi@datarennes.fr"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "https://www.apache.org/licenses/LICENSE-2.0.html"
		}
	},
	"servers": [
		{
			"url": "/{basePath}",
			"description": "Rudi Template API",
			"variables": {
				"basePath": {
					"default": "template/v1"
				}
			}
		}
	],
	"paths": {
	},
	"components": {
		"schemas": {
			"AbstractLongIdDto": {
				"type": "object",
				"properties": {
					"uuid": {
						"type": "string",
						"format": "uuid",
						"readOnly": true
					}
				}
			},
			"AbstractLabelizedDto": {
				"type": "object",
				"required": [
					"code"
				],
				"allOf": [
					{
						"$ref": "#/components/schemas/AbstractLongIdDto"
					},
					{
						"properties": {
							"code": {
								"type": "string",
								"maxLength": 30
							},
							"label": {
								"type": "string",
								"maxLength": 100
							}
						}
					}
				]
			},
			"AbstractStampedDto": {
				"type": "object",
				"required": [
					"opening_date",
					"order"
				],
				"allOf": [
					{
						"$ref": "#/components/schemas/AbstractLabelizedDto"
					},
					{
						"properties": {
							"opening_date": {
								"$ref": "#/components/schemas/RudiDate"
							},
							"closing_date": {
								"$ref": "#/components/schemas/RudiDate"
							},
							"order": {
								"type": "integer"
							}
						}
					}
				]
			},
			"Template": {
				"type": "object",
				"allOf": [
					{
						"$ref": "#/components/schemas/AbstractLongIdDto"
					},
					{
						"properties": {
							"uuid": {
								"type": "string",
								"format": "uuid"
							},
							"comment": {
								"type": "string",
								"description": "comment"
							}
						}
					}
				]
			},
			"SearchCriteria": {
				"description": "Critère de recherche",
				"type": "object",
				"properties": {
					"offset": {
						"description": "Index de début (positionne le curseur pour parcourir les résultats de la recherche)",
						"type": "integer",
						"format": "int32"
					},
					"limit": {
						"description": "Le nombre de résultats à retourner par page",
						"type": "integer",
						"format": "int32"
					}
				}
			},
			"TemplateSearchCriteria": {
				"description": "Critère de recherche d'un template",
				"type": "object",
				"allOf": [
					{
						"$ref": "#/components/schemas/SearchCriteria"
					},
					{
						"properties": {
						}
					}
				]
			},
			"PagedList": {
				"description": "Liste paginée d'éléments",
				"type": "object",
				"required": [
					"total",
					"elements"
				],
				"properties": {
					"total": {
						"description": "Nombre total d'éléments pour cette recherche (peut être supérieur au nombre d'éléments de la page courante)",
						"type": "integer",
						"format": "int64"
					}
				}
			},
			"PagedTemplateList": {
				"description": "Liste paginée de templates",
				"type": "object",
				"allOf": [
					{
						"$ref": "#/components/schemas/PagedList"
					},
					{
						"properties": {
							"elements": {
								"description": "Templates de la page courante pour cette recherche",
								"type": "array",
								"items": {
									"$ref": "#/components/schemas/Template"
								}
							}
						}
					}
				]
			},
			"ReadOnlyDate": {
				"description": "Date in the ISO 8601 format",
				"type": "string",
				"format": "date-time",
				"readOnly": true
			},
			"RudiDate": {
				"description": "Date in the ISO 8601 format",
				"type": "string",
				"format": "date-time"
			}
		}
	}
}

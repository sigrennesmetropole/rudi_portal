{
	"openapi": "3.0.0",
	"info": {
		"title": "${api.getName()}",
		"version": "${api.getVersion()}",
		"termsOfService": "https://swagger.io/terms/",
		"contact": {
			"email": "ca.marchand@rennesmetropole.fr"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "https://www.apache.org/licenses/LICENSE-2.0.html"
		}
	},
	"servers": [
		{
			"url": "/{basePath}",
			"description": "Rudi Konsult API",
			"variables": {
				"basePath": {
					"default": "projekt/v1"
				}
			}
		}
	],
	"tags": [
		{
			"name": "self-data",
			"description": "Services de gestion des self-data"
		}
	],
	"paths": {
		"/*": {
			"get": {
				"tags": [
					"self-data"
				],
				"summary": "Récupération des données de type BarChart",
				"description": "Récupération avec filtrage temporel de données de type graphique barre",
				"parameters": [
					{
						"$ref": "#/components/parameters/MinDateParam"
					},
					{
						"$ref": "#/components/parameters/MaxDateParam"
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/BarChartData"
								}
							}
						}
					},
					"400": {
						"$ref": "#/components/responses/BadRequestError"
					},
					"401": {
						"$ref": "#/components/responses/Unauthorized"
					},
					"403": {
						"$ref": "#/components/responses/Forbidden"
					},
					"406": {
						"$ref": "#/components/responses/NotAcceptable"
					},
					"408": {
						"$ref": "#/components/responses/RequestTimeout"
					},
					"429": {
						"$ref": "#/components/responses/TooManyRequests"
					},
					"500": {
						"$ref": "#/components/responses/InternalServerError"
					},
					"503": {
						"$ref": "#/components/responses/ServiceUnavailable"
					}
				}
			}
		}
	},
	"components": {
		"parameters": {
			"MaxDateParam": {
				"name": "max-date",
				"in": "query",
				"description": "La date maximum",
				"required": false,
				"schema": {
					"type": "string",
					"format": "date-time"
				},
				"allowEmptyValue": false
			},
			"MinDateParam": {
				"name": "min-date",
				"in": "query",
				"description": "La date minimale",
				"required": false,
				"schema": {
					"type": "string",
					"format": "date-time"
				},
				"allowEmptyValue": false
			}
		},
		"schemas": {
			"Language": {
				"description": "Lowercase ISO-639 language code followed by the uppercase 2 letter ISO 3166-1 alpha-2 country code, both joined by an hyphen-minus sign, or alternatively the lowercase ISO-639 language code alone",
				"type": "string",
				"pattern": "^[a-z]{2}(-[A-Z]{2})?$",
				"enum": [
					"s-CZ",
					"da-DK",
					"de-CH",
					"de-DE",
					"en-GB",
					"en-US",
					"el-GR",
					"es-ES",
					"fr-FR",
					"hu-HU",
					"it-IT",
					"no-NO",
					"pl-PL",
					"pt-PT",
					"ro-RO",
					"ru-RU",
					"sk-SK",
					"cs",
					"da",
					"de",
					"en",
					"el",
					"es",
					"fr",
					"hu",
					"it",
					"no",
					"pl",
					"pt",
					"ro",
					"ru",
					"sk"
				]
			},
			"DictionaryEntry": {
				"type": "object",
				"required": [
					"lang",
					"text"
				],
				"properties": {
					"lang": {
						"$ref": "#/components/schemas/Language"
					},
					"text": {
						"type": "string"
					}
				}
			},
			"BarChartType": {
				"description": "Type de graphique de type Barre (affichage des points ou des lignes ou en barre)",
				"type": "string",
				"enum": [
					"POINT",
					"LINE",
					"BAR"
				]
			},
			"BarChartSeries": {
				"description": "Série d'un graphique de type Barre",
				"type": "object",
				"properties": {
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					}
				}
			},
			"BarChartValues": {
				"description": "Valeurs d'une série d'un graphique en barre",
				"type": "object",
				"properties": {
					"colour": {
						"type": "string",
						"description": "la couleur de la barre ou de l'aire sous la courbe soit en #RRGGBBAA soit en nom"
					},
					"borderColour": {
						"type": "string",
						"description": "la couleur du trait de la barre ou de la ligne soit en #RRGGBBAA soit en nom"
					},
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"values": {
						"type": "array",
						"items": {
							"type": "number",
							"format": "double"
						}
					}
				}
			},
			"BarChartData": {
				"description": "Données d'un graphique de type Barre",
				"type": "object",
				"properties": {
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"type": {
						"$ref": "#/components/schemas/BarChartType"
					},
					"legendYAxis": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"legendXAxis": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"series": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/BarChartSeries"
						}
					},
					"values": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/BarChartValues"
						}
					}
				}
			},
			"BubbleChartValues": {
				"description": "Valeur d'un graphique de type nuage de points (position x, y et diamètre du point)",
				"type": "object",
				"properties": {
					"x": {
						"type": "number",
						"format": "double"
					},
					"y": {
						"type": "number",
						"format": "double"
					},
					"r": {
						"type": "number",
						"format": "double"
					}
				}
			},
			"BubbleChartSeries": {
				"description": "Series d'un graphique de type nuage de points",
				"type": "object",
				"properties": {
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"colour": {
						"type": "string",
						"description": "la couleur de la barre ou de l'aire sous la courbe soit en #RRGGBBAA soit en nom"
					},
					"borderColour": {
						"type": "string",
						"description": "la couleur du trait de la barre ou de la ligne soit en #RRGGBBAA soit en nom"
					},
					"values": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/BubbleChartValues"
						}
					}
				}
			},
			"BubbleChartData": {
				"description": "Données d'une graphique de type nuage de points",
				"type": "object",
				"properties": {
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"legendAxisX": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"legendAxisY": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"series": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/BubbleChartSeries"
						}
					}
				}
			},
			"GenericDataType": {
				"description": "Type des champs d'un objet générique",
				"type": "string",
				"enum": [
					"STRING",
					"TEXT",
					"BOOLEAN",
					"LONG",
					"DOUBLE",
					"DATE"
				]
			},
			"GenericData": {
				"description": "Données d'une objet générique",
				"type": "object",
				"properties": {
					"label": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"type": {
						"$ref": "#/components/schemas/GenericDataType"
					},
					"value": {
						"type": "string"
					}
				}
			},
			"GenericDataObject": {
				"description": "Données d'un objet générique",
				"type": "object",
				"properties": {
					"legend": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"data": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/GenericData"
						}
					}
				}
			},
			"ArrayRowData": {
				"description": "Lignes des données tabulaires",
				"type": "object",
				"properties": {
					"value": {
						"type": "string"
					}
				}
			},
			"ArrayDataType": {
				"description": "Types des données tabulaires",
				"type": "string",
				"enum": [
					"BOOLEAN",
					"LONG",
					"DOUBLE",
					"STRING",
					"DATE"
				]
			},
			"ArrayHeaderData": {
				"description": "Entêtes des données tabulaires",
				"type": "object",
				"properties": {
					"name": {
						"description": "Le libellé de l'entête",
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/DictionaryEntry"
						}
					},
					"type": {
						"$ref": "#/components/schemas/ArrayDataType"
					},
					"format": {
						"description": "Le format d'affichage de la colonne selon la norme java pour les numériques et les dates",
						"type": "string"
					}
				}
			},
			"ArrayData": {
				"description": "Données tabulaire",
				"type": "object",
				"properties": {
					"headers": {
						"description": "Les entêtes",
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/ArrayHeaderData"
						}
					},
					"data": {
						"description": "Les lignes de données",
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/ArrayRowData"
						}
					}
				}
			},
			"ReadOnlyDate": {
				"description": "Date in the ISO 8601 format",
				"type": "string",
				"format": "date-time",
				"readOnly": true
			},
			"RudiDate": {
				"description": "Date in the ISO 8601 format",
				"type": "string",
				"format": "date-time"
			},
			"ApiError": {
				"type": "object",
				"properties": {
					"code": {
						"type": "string"
					},
					"label": {
						"type": "string"
					}
				}
			}
		},
		"responses": {
			"InternalServerError": {
				"description": "Internal server error"
			},
			"BadRequestError": {
				"description": "Bad Request Error",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"Unauthorized": {
				"description": "Unauthorized",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"Forbidden": {
				"description": "Forbidden",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"NotAcceptable": {
				"description": "Not Acceptable",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"RequestTimeout": {
				"description": "Request Timeout",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"TooManyRequests": {
				"description": "Too Many Requests",
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/ApiError"
						}
					}
				}
			},
			"ServiceUnavailable": {
				"description": "Service Unavailable"
			}
		}
	},
	"x-wso2-auth-header": "Authorization",
	"x-wso2-cors": {
		"corsConfigurationEnabled": false,
		"accessControlAllowOrigins": [
			"*"
		],
		"accessControlAllowCredentials": false,
		"accessControlAllowHeaders": [
			"authorization",
			"Access-Control-Allow-Origin",
			"Content-Type",
			"SOAPAction",
			"apikey",
			"testKey"
		],
		"accessControlAllowMethods": [
			"GET",
			"PUT",
			"POST",
			"DELETE",
			"PATCH",
			"OPTIONS"
		]
	},
	"x-wso2-production-endpoints": {
		"urls": [
			"${api.getEndpointUrl()}"
		],
		"type": "http"
	},
	"x-wso2-sandbox-endpoints": {
		"urls": [
			"${api.getEndpointUrl()}"
		],
		"type": "http"
	},
	"x-wso2-basePath": "${api_context}/${api.getVersion()}",
	"x-wso2-transports": [
		"http",
		"https"
	],
	"x-wso2-response-cache": {
		"enabled": false,
		"cacheTimeoutInSeconds": 300
	}
}

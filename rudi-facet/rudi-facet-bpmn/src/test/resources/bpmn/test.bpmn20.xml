      <bpmn:incoming>SequenceFlow_4</bpmn:incoming>
      <bpmn:incoming>SequenceFlow_16</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_6</bpmn:outgoing>
      <bpmn:script>
        workflowContext.info(&quot;Update status canceled&quot;);
        workflowContext.updateStatus(context, execution, &quot;CANCELLED&quot;, &quot;Demande abandonnée&quot;);
        var localEmailData = new org.rudi.facet.bpmn.bean.workflow.EMailData(&quot;stl:Abandon d'une demande&quot;, &quot;file::templates/initiator-cancelled-mail.html&quot;);
        workflowContext.sendEMailToInitiator(context,execution,localEmailData);

        "ng-openapi-gen": "^0.12.1",
        "node-sass-install": "^1.0.2",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}

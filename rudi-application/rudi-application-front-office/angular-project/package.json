{
    "name": "angular-project",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json",
        "start-prod": "ng serve --prod --proxy-config proxy.conf.json",
        "build-prod": "ng build --prod",
        "build-dev": "ng build",
        "build-${env}": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "generate:mydy": "openapi-generator-cli help",
        "generate:help": "openapi-generator-cli config-help -g typescript-angular",
        "generate:all": "npm run generate:all-facets && npm run generate:all-microservices",
        "generate:all-facets": "npm run generate:kmedia && npm run generate:kaccess && npm run generate:bpmn && npm run generate:rva && npm run generate:apimaccess",
        "generate:apimaccess": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-apimaccess/src/main/resources/openapi/rudi-apimaccess-model.json -o src/app/api-apimaccess",
        "generate:bpmn": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-bpmn/openapi/rudi-bpmn-model.json -o src/app/api-bpmn",
        "generate:kaccess": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-kaccess/src/main/resources/openapi/rudi-kaccess-model.yml -o src/app/api-kaccess",
        "generate:kmedia": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-kmedia/src/main/resources/openapi/rudi-kmedia-model.yml -o src/app/api-kmedia",
        "generate:rva": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-facet/rudi-facet-rva/openapi/rudi-rva-model.json -o src/app/api-rva",
        "generate:all-microservices": "npm run generate:strukture && npm run generate:konsult && npm run generate:acl && npm run generate:kos && npm run generate:projekt && npm run generate:selfdata && npm run generate:konsent",
        "generate:strukture": "npm run generate:strukture-model && npm run generate:strukture-api",
        "generate:strukture-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-strukture/openapi/rudi-strukture-api.json -o src/app/strukture/api-strukture -c openapitools.json -t ./template",
        "generate:strukture-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-strukture/openapi/rudi-strukture-model.json -o src/app/strukture/strukture-model",
        "generate:acl": "npm run generate:acl-model && npm run generate:acl-api",
        "generate:acl-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-acl/openapi/rudi-acl-api.json -o src/app/acl/acl-api -t ./template",
        "generate:acl-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-acl/openapi/rudi-acl-model.json -o src/app/acl/acl-model",
        "generate:konsult": "npm run generate:konsult-model && npm run generate:konsult-api",
        "generate:konsult-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-konsult/openapi/rudi-konsult-model.json -o src/app/konsult/konsult-model -c openapitools.json -t ./template",
        "generate:konsult-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-konsult/openapi/rudi-konsult-api.json -o src/app/konsult/konsult-api -c openapitools.json -t ./template",
        "generate:kos": "npm run generate:kos-model && npm run generate:kos-api",
        "generate:kos-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-kos/openapi/rudi-kos-api.json -o src/app/kos/kos-api -t ./template",
        "generate:kos-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-kos/openapi/rudi-kos-model.json -o src/app/kos/kos-model",
        "generate:projekt": "npm run generate:projekt-model && npm run generate:projekt-api",
        "generate:projekt-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-projekt/openapi/rudi-projekt-api.json -o src/app/projekt/projekt-api -c openapitools.json -t ./template",
        "generate:projekt-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-projekt/openapi/rudi-projekt-model.json -o src/app/projekt/projekt-model -c openapitools.json -t ./template",
        "generate:selfdata": "npm run generate:selfdata-model && npm run generate:selfdata-api",
        "generate:selfdata-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-selfdata/openapi/rudi-selfdata-api.json -o src/app/selfdata/selfdata-api -c openapitools.json -t ./template",
        "generate:selfdata-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-selfdata/openapi/rudi-selfdata-model.json -o src/app/selfdata/selfdata-model -c openapitools.json -t ./template",
        "generate:konsent": "npm run generate:konsent-model && npm run generate:konsent-api",
        "generate:konsent-api": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-konsent/openapi/rudi-konsent-api.json -o src/app/konsent/konsent-api -c openapitools.json -t ./template",
        "generate:konsent-model": "openapi-generator-cli generate -g typescript-angular -i ../../../rudi-microservice/rudi-microservice-konsent/openapi/rudi-konsent-model.json -o src/app/konsent/konsent-model -c openapitools.json -t ./template"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.0",
        "@angular/cdk": "^11.2.13",
        "@angular/common": "~11.0.0",
        "@angular/compiler": "~11.0.0",
        "@angular/core": "~11.0.0",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "~11.0.0",

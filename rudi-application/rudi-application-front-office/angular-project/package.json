        "ag-grid-angular": "^27.2.1",
        "ag-grid-community": "^27.2.1",
        "angular-captcha": "^3.7.4",
        "angular-svg-icon": "^11.0.0",
        "bootstrap": "^4.5.3",
        "cropperjs": "^1.5.12",
        "d3": "^6.7.0",
        "file-saver": "^2.0.5",
        "font-awesome": "^4.7.0",

        "query-string": "^6.14.0",
        "rxjs": "^6.6.3",
        "sass": "^1.42.1",
        "tslib": "^2.0.0",
        "uuid": "^9.0.0",
        "xlsx": "^0.18.5",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.0",
        "@angular/cli": "~11.0.0",
        "@angular/compiler-cli": "~11.0.0",
        "@openapitools/openapi-generator-cli": "2.1.18",
        "@types/jasmine": "~3.6.0",
